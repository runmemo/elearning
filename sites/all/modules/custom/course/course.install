<?php

require_once drupal_get_path('module', 'course') . '/course.user_role.inc';

function course_install() {

    foreach (_course_user_default_roles() as $role) {
        $role = (object) $role;
        if ($existing = user_role_load_by_name($role->name)) {
          $role->rid = $existing->rid;
        }
        user_role_save($role);  
    }   
}

